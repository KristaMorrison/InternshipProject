# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore Mac Custom Attributes File
.DS_store

# Ignore Uploaded Paperclip Images
/public/system/*

# Ignore Personal Database Setup
/config/database.yml
